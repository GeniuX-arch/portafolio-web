---
import Icons from "./Icons.astro"
import Image from "astro/components/Image.astro";

interface Props {
  list: ImageMetadata[];
  width: number;
}

const { list,width } = Astro.props;
---


    <ul class="flex flex-row justify-center w-10/12 flex-wrap">
        {
        list.map((name)=>
            <Icons hover="" link={name} name="" width={width}/>
        )
        }
    </ul>