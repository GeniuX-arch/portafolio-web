---
import Icons from "./Icons.astro"
interface Props {
  list: string[];
  width: String;
}

const { list,width } = Astro.props;
---


    <ul class="flex flex-row justify-center w-10/12 flex-wrap">
        {
        list.map((name)=>
            <Icons hover="" link=`/public/tecnologies/${name}.svg` name="" witdth={width}/>
        )
        }
    </ul>